{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "label": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "label",
      "required": true
    },
    "home_page": {
      "type": "boolean",
      "default": false
    },
    "path": {
      "type": "string"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.seo",
        "blocks.script",
        "blocks.text-block",
        "blocks.deck-block",
        "blocks.media-block",
        "blocks.search-sort-block"
      ]
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page.page",
      "inversedBy": "children"
    },
    "children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page",
      "mappedBy": "parent"
    },
    "static_path": {
      "type": "json"
    }
  }
}
